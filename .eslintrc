{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "ignore-path": ".eslintignore",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
   },
   "ecmaFeatures": {
    "jsx": true,
    "es6": true,
    "classes": true,
   },
   "rules": {
     "comma-dangle": [1, "always-multiline"],
     "max-len": 0,
     "arrow-body-style": [0],
     "no-console": 0,
     "no-underscore-dangle": 0,
     "consistent-return": 0,
     "no-multiple-empty-lines": 0,
     "no-param-reassign": 0,
     "no-use-before-define": 0,
     "react/prop-types": 0,
     "react/prefer-stateless-function": 0,
     "react/jsx-no-bind": 0,
     "jsx-a11y/href-no-hash": "off",
     "jsx-a11y/anchor-is-valid": ["warn", { "aspects": ["invalidHref"] }],
     "export/prefer-default-export": 0
   },
}
